<?xml version="1.0" encoding="UTF-8" ?>
<content-type>
	<display-name>Resultat</display-name>
	<display-name-expression>${title}</display-name-expression>
	<description>Veiviser</description>
	<super-type>base:structured</super-type>
	<allow-child-content-type>${app}:*</allow-child-content-type>
	<allow-child-content-type>base:folder</allow-child-content-type>
	<form>
		<item-set name="group">
			<label>Grupper</label>
			<occurrences minimum="0" maximum="0" />
			<items>
				<input name="result" type="ContentSelector">
					<label>Resultat</label>
					<occurrences minimum="1" maximum="0" />
					<config>
						<allowContentType>result-engine-result</allowContentType>
						<allowPath>./*</allowPath>
					</config>
				</input>
			</items>
		</item-set>
	</form>
</content-type>
