<content-type>
	<display-name i18n="wizardTag.displayname">Svargruppe</display-name>
	<display-name-expression>${text}</display-name-expression>
	<display-name-label i18n="wizardTag.displayname.label">Svargruppe</display-name-label>
	<description
		i18n="wizardTag.description">Gruppe av svar som kan refereres til dersom man bruker oppsummering</description>
	<super-type>base:structured</super-type>
	<allow-child-content-type>${app}:answer*</allow-child-content-type>

	<form>
		<input name="text" type="TextLine">
			<label>Svartekst</label>
			<occurrences maximum="1" minimum="1" />
		</input>
	</form>
</content-type>
