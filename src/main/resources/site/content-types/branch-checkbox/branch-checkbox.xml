<content-type>
	<display-name i18n="branchCheckbox.displayname">Gren sjekkboks</display-name>
	<display-name-label
		i18n="branchCheckbox.displayname.label">Gren med sjekkboks-svar</display-name-label>
	<description i18n="branchCheckbox.description">Gren med sjekkboks-svar</description>
	<super-type>base:structured</super-type>
	<allow-child-content-type>${app}:result</allow-child-content-type>
	<allow-child-content-type>${app}:question</allow-child-content-type>
	<allow-child-content-type>${app}:answer*</allow-child-content-type>
	<allow-child-content-type>${app}:result-engine</allow-child-content-type>
	<allow-child-content-type>base:folder</allow-child-content-type>

	<form>
		<mixin name="answers" />
		<mixin name="next-question-or-result" />
	</form>
</content-type>
