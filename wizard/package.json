{
  "author": "Mattilsynet",
  "description": "Veiviser for Mattilsynet",
  "private": false,
  "homepage": "https://github.com/Mattilsynet",
  "gitRepository": "https://github.com/Mattilsynet",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "concurrently -c auto -g --timings npm:build:*",
    "minify": "concurrently -c auto -g --timings 'npm:build:browser -- --minify' npm:build:sass 'npm:build:server -- --minify' 'npm:build:static -- --minify'",
    "build:server": "npx tsup -d build/resources/main",
    "build:static": "npx tsup -d build/resources/main/static",
    "check": "concurrently -c auto -g --timings npm:lint npm:check:types",
    "check:types": "concurrently -g -r --timings npm:check:types:*",
    "check:types:server": "npx tsc --noEmit",
    "check:types:static": "npx tsc --noEmit -p src/main/resources/static/tsconfig.json",
    "lint": "eslint --cache src/main/resources/**/*.ts",
    "test": "jest --no-cache --coverage",
    "watch": "concurrently -c auto npm:watch:*",
    "watch:browserSync": "npx browser-sync start --files \"src/main/resources/**/*.html\" \"src/main/resources/**/*.xml\" \"build/resources/main\" --reload-delay 0 --port 3100 --no-snippet --watch",
    "watch:sass": "npm run build:sass -- --watch",
    "watch:server": "npm run build:server -- --watch"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@enonic/js-utils": "1.8.0",
    "dayjs": "1.11.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@enonic-types/global": "7.14.4",
    "@enonic-types/lib-admin": "7.14.4",
    "@enonic-types/lib-app": "^7.14.4",
    "@enonic-types/lib-auditlog": "^7.14.4",
    "@enonic-types/lib-auth": "^7.14.4",
    "@enonic-types/lib-cluster": "^7.14.4",
    "@enonic-types/lib-common": "^7.14.4",
    "@enonic-types/lib-content": "^7.14.4",
    "@enonic-types/lib-context": "^7.14.4",
    "@enonic-types/lib-event": "^7.14.4",
    "@enonic-types/lib-export": "^7.14.4",
    "@enonic-types/lib-grid": "^7.14.4",
    "@enonic-types/lib-i18n": "^7.14.4",
    "@enonic-types/lib-io": "^7.14.4",
    "@enonic-types/lib-mail": "^7.14.4",
    "@enonic-types/lib-node": "^7.14.4",
    "@enonic-types/lib-portal": "^7.14.4",
    "@enonic-types/lib-project": "^7.14.4",
    "@enonic-types/lib-repo": "^7.14.4",
    "@enonic-types/lib-scheduler": "^7.14.4",
    "@enonic-types/lib-schema": "^7.14.4",
    "@enonic-types/lib-task": "^7.14.4",
    "@enonic-types/lib-value": "^7.14.4",
    "@enonic-types/lib-vhost": "^7.14.4",
    "@enonic-types/lib-websocket": "^7.14.4",
    "@enonic/esbuild-plugin-copy-with-hash": "^0.2.0",
    "@enonic/mock-xp": "^0.10.0",
    "@enonic/tsup-plugin-manifest": "^0.0.1",
    "@item-enonic-types/lib-graphql": "^2.0.3",
    "@jest/globals": "^29.7.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/react": "18.3.12",
    "browser-sync": "3.0.3",
    "concurrently": "8.2.2",
    "esbuild-plugin-globals": "^0.2.0",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "glob": "^10.4.5",
    "jest": "^29.7.0",
